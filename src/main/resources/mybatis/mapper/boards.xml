<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.mapper.boards">
	<select id="selectAll" resultType="boards">
		select 
			bno,
			btitle,
			bwriter,
			bhitcount,
			bdate
		from boards
		
	</select>
</mapper>